{
  "name": "@aha-develop/zendesk",
  "description": "Zendesk integration extension",
  "version": "1.1.1",
  "author": "Aha! (support@aha.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/aha-develop/zendesk"
  },
  "license": "MIT",
  "ahaExtension": {
    "cspSources": [
      "https://*.zendesk.com"
    ],
    "contributes": {
      "commands": {
        "addLink": {
          "entryPoint": "src/commands/addLink.js",
          "title": "Zendesk: Link ticket to record"
        }
      },
      "importers": {
        "import": {
          "title": "Zendesk",
          "entryPoint": "src/import.js"
        }
      },
      "settings": {
        "subdomain": {
          "title": "Subdomain",
          "description": "The subdomain for your Zendesk account, where subdomain is https://{subdomain}.zendesk.com/",
          "scope": [
            "account"
          ],
          "type": "string"
        }
      },
      "views": {
        "tickets": {
          "title": "Zendesk dashboard",
          "entryPoint": "src/tickets.js",
          "host": "page",
          "location": {
            "menu": "Work"
          }
        }
      }
    }
  },
  "devDependencies": {
    "aha-cli": "^1.9.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@aha-app/aha-develop-react": "^1.0.5"
  }
}
